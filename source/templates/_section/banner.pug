section#banner
    #banner-hero.carousel.slide(data-bs-ride='carousel')
        .carousel-indicators
            button.active(type='button' data-bs-target='#banner-hero' data-bs-slide-to='0' aria-current='true' aria-label='Slide 1')
            button(type='button' data-bs-target='#banner-hero' data-bs-slide-to='1' aria-label='Slide 2')
            button(type='button' data-bs-target='#banner-hero' data-bs-slide-to='2' aria-label='Slide 3')
            button(type='button' data-bs-target='#banner-hero' data-bs-slide-to='3' aria-label='Slide 4')
            button(type='button' data-bs-target='#banner-hero' data-bs-slide-to='4' aria-label='Slide 5')
            button(type='button' data-bs-target='#banner-hero' data-bs-slide-to='5' aria-label='Slide 6')
            button(type='button' data-bs-target='#banner-hero' data-bs-slide-to='6' aria-label='Slide 7')
            button(type='button' data-bs-target='#banner-hero' data-bs-slide-to='7' aria-label='Slide 8')

        .carousel-inner
            .carousel-item.active
                img.d-block.w-100(src="/images/banners/enjoy-cabo-01.png" alt="")
            .carousel-item
                img.d-block.w-100(src="/images/banners/enjoy-cabo-02.png" alt="")
            .carousel-item
                img.d-block.w-100(src="/images/banners/enjoy-cabo-03.png" alt="")
            .carousel-item
                img.d-block.w-100(src="/images/banners/enjoy-cabo-04.png" alt="")
            .carousel-item
                img.d-block.w-100(src="/images/banners/enjoy-cabo-05.png" alt="")
            .carousel-item
                img.d-block.w-100(src="/images/banners/enjoy-cabo-06.png" alt="")
            .carousel-item
                img.d-block.w-100(src="/images/banners/enjoy-cabo-07.png" alt="")
            .carousel-item
                img.d-block.w-100(src="/images/banners/enjoy-cabo-08.png" alt="")
                    
        button.carousel-control-prev(type='button' data-bs-target='#banner-hero' data-bs-slide='prev')
            span.carousel-control-prev-icon(aria-hidden='true')
            span.visually-hidden Previous
        button.carousel-control-next(type='button' data-bs-target='#banner-hero' data-bs-slide='next')
            span.carousel-control-next-icon(aria-hidden='true')
            span.visually-hidden Next
